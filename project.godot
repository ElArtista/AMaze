; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="AMaze"
run/main_scene="res://scenes/menu/MainMenu.tscn"
config/icon="res://assets/icon.png"

[autoload]

global="*res://global.gd"

[display]

window/size/width=1280
window/size/height=800
window/size/resizable=false
window/size/test_width=1280
window/size/test_height=600
window/handheld/emulate_touchscreen=true
window/stretch/mode="2d"
window/stretch/shrink="1"

[editor_plugins]

enabled=PoolStringArray( "vnen.tiled_importer" )

[layer_names]

2d_physics/layer_1="player"
2d_physics/layer_2="item"
2d_physics/layer_3="wall"
2d_physics/layer_4="path_point"

[rendering]

environment/default_environment="res://default_env.tres"
